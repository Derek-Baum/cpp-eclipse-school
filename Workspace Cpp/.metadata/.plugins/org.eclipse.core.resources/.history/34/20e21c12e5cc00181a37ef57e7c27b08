#include <iostream>
#include "LLPQ.hpp"

using namespace std;

LLPQ::LLPQ(){
	first = NULL;
	last = NULL;
	size = 0;
}
LLPQ::~LLPQ(){
	LLNode *tmp = first;
	while(tmp!= NULL){
		LLNode *next = tmp->right;
		tmp->~LLNode();
		tmp = next;
	}
	cout<<"Deleted each node in ll"<<endl;
}
void LLPQ::printLLPQ(){
	LLNode *tmp = first;
	while(tmp!=NULL){
		tmp->printNode();
		cout<<endl;
		tmp = tmp->right;
	}
	cout<<endl;
}
void LLPQ::addFirst(char x, string co){
	first = new LLNode(x,"-1");
	size++;
}
void LLPQ::addAtFirst(char x, string co){
	first->left = new LLNode(x,co);
	first->left = first;
	size++;
}

LLNode* LLPQ::remFirst(){
	LLNode *ret = first;
	first = first->right;
	first->left = NULL;
	size--;

	return ret;
}
string findCode(char k){
	LLNode *tmp = first;
}
