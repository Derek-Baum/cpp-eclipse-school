#include <iostream>
#include "LLPQ.hpp"
#include "LLNode.hpp"
#include <stdlib.h>


using namespace std;


int main(){

	LLPQ test;
	test.addFirst('B',"2",2);
//	test.printLLPQ();

	test.addAtFirst('A',"1",1);
//	test.printLLPQ();


	test.addAtFirst('C',"3",3);
//	test.printLLPQ();


	test.addAtFirst('E',"5",5);
	test.printLLPQ();

	cout<<"now we sort it"<<endl;

	test.sortLL();
	cout<<"Sorted list"<<endl;
	test.printLLPQ();

	LLNode *toAdd = new LLNode('a',"0",0);
	test.insertInOrder(toAdd);
	test.printLLPQ();


	test.insertInOrder(new LLNode('a',"0",0));
	test.printLLPQ();
	return 0;
}


