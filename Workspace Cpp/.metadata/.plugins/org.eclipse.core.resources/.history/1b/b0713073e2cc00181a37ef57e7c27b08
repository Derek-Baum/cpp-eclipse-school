#include <iostream>
#include "LLPQ.hpp"

using namespace std;

LLPQ::LLPQ(){
	first = NULL;
	size = 0;
}
LLPQ::~LLPQ(){
	LLNode *tmp = first;
	while(tmp!= NULL){
		LLNode *next = tmp->right;
		tmp->~LLNode();
		tmp = next;
	}
}
