#include <iostream>
using namespace std;
void modifyX(int *x);
void modifyRef(int &x);

int main() {
	int k = 5;
	int *x = &k;
	cout<<x<<endl;
	cout<<&k<<endl;
	*x =10;
	cout<<k<<endl;
	int *y = x;
	cout<<y<<endl;

	return 0;
}

// call by pointer
// you are giving the function the address variable
void modifyX(int *x){
	*x = 9;
}

// call by reference
// you are assigning a new variable to the memory address that is passed in
// this is java

void modifyRef(int &x){
	x = 900;
}
