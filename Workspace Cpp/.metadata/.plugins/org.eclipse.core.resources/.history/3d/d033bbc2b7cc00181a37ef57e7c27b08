#include "LLNode.hpp"

#include <iostream>

using namespace std;

SNode::SNode(char character, int freq){
	c = character;
	frequency = freq;
	code = "";
	left=right=NULL;
}
SNode::~SNode(){
	cout<<"Deleting "<<c<<endl;
}
void SNode::printNode(){
	cout<<"Character "<<c<<endl<<"Frequency: "<<frequency<<endl<<"Code: "<<code<<endl;
}
