#include <iostream>
#include "LLPQ.hpp"
#include "LLNode.hpp"
#include <stdlib.h>
#include <fstream>
#include "LLHuff.hpp"


using namespace std;


int main(){
	cout<<"reading file"<<endl;
	LLHuff code("test.txt");
	code.ReadFile();
	code.pq->printLLPQ();
	code.pq->sortLL();
	code.pq->printLLPQ();
	cout<<"we get to the end of the code."<<endl;
	code.MakeHuff();
	cout<<"we get to the end of the code2."<<endl;
	code.compressFile();
	cout<<"we get to the end of the code3."<<endl;
	code.ReadAscii();

	return 0;
}


