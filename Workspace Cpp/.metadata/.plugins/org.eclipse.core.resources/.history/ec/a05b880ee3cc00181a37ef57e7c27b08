#include <iostream>
#include "LLPQ.hpp"

using namespace std;

LLPQ::LLPQ(){
	first = NULL;
	last = NULL;
	size = 0;
}
LLPQ::~LLPQ(){
	LLNode *tmp = first;
	while(tmp!= NULL){
		LLNode *next = tmp->right;
		tmp->~LLNode();
		tmp = next;
	}
	cout<<"Deleted each node in ll"<<endl;
}
void LLPQ::printLLPQ(){
	LLNode *tmp = first;
	while(tmp!=NULL){
		tmp->printNode();
		cout<<endl;
		tmp = tmp->right;
	}
	cout<<endl;
}
