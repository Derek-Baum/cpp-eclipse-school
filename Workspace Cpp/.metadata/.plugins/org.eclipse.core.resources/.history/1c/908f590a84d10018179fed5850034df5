#include <iostream>
#include "LLHuff.hpp"

using namespace std;

LLHuff::LLHuff(string f){
	file = f;
	root = NULL;
	size = 0;
	pq = NULL;
	ascii = NULL;
}
LLHuff::~LLHuff(){
	delete root;
	delete pq;
	delete ascii;
}
